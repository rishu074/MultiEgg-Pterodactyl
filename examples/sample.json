                    "value": 1,
                    "href": "{{NODEJS_HREF}}",
                    "type": "andea",
                    "textColor": "whiteBright",
                    "top": [
                        {
                            "text": "Choose nodejs version",
                            "textColor": "whiteBright"
                        }
                    ],
                    "scripts": [
                        "env(&?&)node(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/node",
                        "env(&?&)NODE(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/node",
                        "env(&?&)NPM(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/npm",
                        "pathFinder(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/node(&?&)NODEJS_HREF(&?&)nodejsStartIndexjs(&?&)InstallNodejsTest"
                    ]
                }
            ],
            "config_variable": "node_version_selected"
        }
    },
    "andeas": {
        "InstallNodejsTest": {
            "href": "InstallNPM",
            "hrefType": "andea",
            "type": "d",
            "scripts": [
                "env(&?&)node(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/node",
                "env(&?&)NODE(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/node",
                "env(&?&)NPM(&?&)/home/container/.startup/node-v18.12.1-linux-x64/bin/npm"
            ],
            "exec": [
                {
                    "message": "Installing nodejs",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/ && rm -rf .startup",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "command": "cd /home/container/ && mkdir .startup",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "command": "cd /home/container/.startup && wget -O node.tar.xz https://nodejs.org/dist/v18.12.1/node-v18.12.1-linux-x64.tar.xz && tar -xf node.tar.xz && rm node.tar.xz",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "command": "cd /home/container/ && mkdir node_modules",
                    "output": false,
                    "outputColor": "white"
                }
            ]
        },
        "InstallNPM": {
            "href": "nodejsStartIndexjs",
            "hrefType": "andea",
            "type": "d",
            "exec": [
                {
                    "message": "Installing Modules",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/ && {{NPM}} install",
                    "output": true,
                    "outputColor": "whiteBright"
                }
            ],
            "env": {
                "PATH": "/home/container/.startup/node-v18.12.1-linux-x64/bin",
                "HOME": "/home/container"
            }
        },
        "InstallRequirmentsPython": {
            "href": "PythonUserScreen",
            "type": "d",
            "hrefType": "page",
            "exec": [
                {
                    "message": "Installing requirments (This is a one time process, it make take few minutes.)",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/ && {{PIP}} install -r requirements.txt",
                    "output": true,
                    "outputColor": "whiteBright"
                }
            ]
        },
        "InstallPythonTest": {
            "href": "PythonUserScreen",
            "hrefType": "page",
            "type": "d",
            "scripts": [
                "env(&?&)HOME(&?&)/home/container",
                "env(&?&)PYTHON(&?&)/home/container/.startup/python/bin/python3/bin/python3",
                "env(&?&)PIP(&?&)/home/container/.startup/python/bin/python3/bin/pip3"
            ],
            "exec": [
                {
                    "message": "Downloading Python (This is a one time process, it make take few minutes.)",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/ && rm -rf .startup",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "command": "cd /home/container/ && mkdir .startup",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "command": "cd /home/container/.startup && wget -O python.tgz https://www.python.org/ftp/python/3.11.1/Python-3.11.1.tgz && tar -xzf python.tgz && rm python.tgz",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "command": "mkdir {{PYTHON}}",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "message": "Installing Python",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/.startup/Python-3.11.1 && ./configure --prefix={{PYTHON}} --enable-optimizations",
                    "output": true,
                    "outputColor": "white"
                },
                {
                    "message": "Building Python (May take few minutes...)",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/.startup/Python-3.11.1 && make",
                    "output": false,
                    "outputColor": "white"
                },
                {
                    "message": "Finalizing Python",
                    "textColor": "whiteBright",
                    "command": "cd /home/container/.startup/Python-3.11.1 && make install",
                    "output": false,
                    "outputColor": "white"
                }
            ]
        },
        "PythonStartServer": {
            "type": "a",
            "stop": "SIGINT",
            "exec": {
                "command": "cd /home/container/ && {{PYTHON}} {{PYTHON_FILE}}"
            },
            "consoleColor": "whiteBright",
            "consoleErrorColor": "red"
        },
        "nodejsStartIndexjs": {
            "type": "a",
            "stop": "SIGINT",
            "scripts": [
                "env(&?&)HOME(&?&)/home/container",
                "pathFinder(&?&)/home/container/index.loda(&?&)IS_INDEX(&?&)YES(&?&)NO",
                "env(&?&)LOL_TEST(&?&){{IS_INDEX}}"
            ],
            "exec": {
                "command": "cd /home/container/ && {{NODE}} index.js"
            },
            "env": {
                "TEST_222": "{{LOL_TEST}}",
                "PATH": "{{PATH}}:/home/container/.startup/node-v18.12.1-linux-x64/bin"
            },
            "consoleColor": "whiteBright",
            "consoleErrorColor": "red"
        }
    }
}